{
  "name": "Browser Sharing",
  "description": "Extension that allows users to share a browser.",
  "version": "1.0",
  "permissions": [
    "activeTab", "tabs", "http://*/"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts":[{
        "js": ["content_script.js"],
        "matches": ["http://*/*", "https://*/*"]
  }],
  "browser_action": {
    "default_title": "Initialize Browser Sharing"
  },
  "manifest_version": 2
}
